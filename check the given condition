#include<stdio.h>
int main(){
    char s[100];
    scanf("%[^\n]",s);
    int i,v=0,c=0,sp=0,d=0;
    for(int i=0;s[i]!='\0';i++){
        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')
        v++;
        else if(s[i]==' ')
        sp++;
        else if(s[i]>='0'&&s[i]<='9')
        d++;
        else
        c++;
    }
    printf("Vowels=%d\nSpace=%d\nDigits=%d\nConstrand=%d\n",v,sp,d,c);
    return 0;
    
}
